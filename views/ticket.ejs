<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>D.I.P.E.R Token</title>
    <link rel="stylesheet" href="/stylesheets/ticket_style.css">
     
</head>
<body>
    <div class="container">
        <header>
            <div class="logo">
                <h1>D.I.P.E.R</h1>
            </div>
            <div class="logout">
                <a href="/logout">Logout</a>
            </div>
        </header>
        
        <main>
            <div class="token-box">
                <h2>D.I.P.E.R Token</h2>
                <div class="date-time">
                    <h3>Date & Time</h3>
                    <p id="currentDateTime"><%= time%></p>
                </div>
                <div class="option-choosed">
                    <h3>Option Chosen</h3>
                    <p id="chosenOption"><%= option%></p>
                </div>
            </div>
        </main>
    </div>

  
</body>

